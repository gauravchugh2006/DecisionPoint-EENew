@model DecisionPoint.Models.UserDashBoard
@using DecisionPoint.Models.DashBoardViewModel.ViewModel;
@using DecisionPointCAL;
@using DecisionPointCAL.Common;
@using DecisionPointBAL.Implementation.Response;
@using System.Globalization
@{
    ViewBag.Title = "ClientViewByIC";
    Layout = "~/Views/Shared/_CommonLayout.cshtml";
    string reqCompanyId = string.Empty;
    int SterlingPkgId = 0;
}


  <div class="row-fluid">
 <div class="span12">
          <a class="btn btn-primary pull-right" style="margin-bottom:10px;" id="btnClose">Close</a>
      </div>
      </div>
  <div class="row-fluid">
     
                @if (!Convert.ToString(Session["UserType"], CultureInfo.InvariantCulture).Equals(Shared.NonClient))
                {
                    
                     
                    <div class="row-fluid">
                        <div class="box background-check">
                            @if (Model.IsBgCheckApply || Model.IsLicInspply)
                            {
                                if (Model.IsBgCheckApply)
                                {
                                <div class="row-fluid">
                                    <div class="span12">
                                        <h3>Background Check</h3>

                                    </div>
                                </div>
                                <div class="box background-check">
                                    <table id="tbl_BackgroundCheck" class="table table-hover insurance-table">
                                        <thead>
                                            <tr>
                                                
                                                    <th>Required By</th>
                                                
                                                <th>Title</th>
                                                <th>Source</th>
                                                <th>Received</th>
                                                <th style="text-align: center;">Link</th>
                                                <th>Status</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            @if (Model.BackgroundList != null)
                                            {
                                                foreach (var item in Model.BackgroundList)
                                                {
                                                    if (reqCompanyId != item.RequiredByCompanyId || SterlingPkgId != item.SterlingPkgId)
                                                    {
                                                <tr>
                                                   
                                                        <td>@item.RequiredByCompanyName
                                                        </td>
                                                    
                                                    <td>@if (reqCompanyId != item.RequiredByCompanyId || SterlingPkgId != item.SterlingPkgId)
                                                        {
                                                        @item.BackgroundTitle
                                                        }
                                                    </td>
                                                    <td>@if (!string.IsNullOrEmpty(item.Source))
                                                        {@item.Source.Split(char.Parse(Shared.Comma))[0]}</td>
                                                    <td>@if (item.ReceivedDate != null)
                                                        {
                                                        @item.ReceivedDate.Value.ToShortDateString()
                                                        }
                                                    </td>
                                                    <td style="text-align: center;">
                                                        @if (reqCompanyId != item.RequiredByCompanyId || SterlingPkgId != item.SterlingPkgId)
                                                        {
                                                            if (!string.Equals(item.BackgroundTitle, "Basic"))
                                                            {
                                                                
                                                            <a id="@item.SterlingOrderId" style="cursor:pointer;" onclick="ReviewSterlingResult('@item.SterlingOrderId')">Review Results</a>
                                                                //<a style="cursor:pointer;" onclick="ABC('@item.SterlingPkgId','@item.SterlingPkgName')">Review Results</a>
                                                            }
                                                        }
                                                    </td>
                                                    <td>
                                                        @if (reqCompanyId != item.RequiredByCompanyId || SterlingPkgId != item.SterlingPkgId)
                                                        {
                                                            <span>@item.Status</span>
                                                            
                                                        }
                                                        else
                                                        {
                                                            <p>&nbsp;</p>
                                                        }


                                                    </td>
                                                   
                                                </tr>
                                                
                                                <tr>
                                                   
                                                        <td></td>
                                                    
                                                    <td style="padding: 0px 5px;">@if (!string.IsNullOrEmpty(item.Description))
                                                                                  {<span style="font-style: italic; font-size: 12px;">@item.Description</span>}
                                                                                  else
                                                                                  {<span>-</span>}
                                                    </td>
                                                    <td></td>
                                                    <td></td>
                                                    <td></td>
                                                    <td></td>
                                                    <td></td>
                                                  
                                                </tr>
                                                    }
                                                    else
                                                    {
                                                <tr>
                                                   
                                                        <td></td>
                                                    
                                                    <td style="padding: 0px 5px;">@if (!string.IsNullOrEmpty(item.Description))
                                                                                  {<span style="font-style: italic; font-size: 12px;">@item.Description</span>}
                                                                                  else
                                                                                  {<span>-</span>}
                                                    </td>
                                                    <td></td>
                                                    <td></td>
                                                    <td></td>
                                                    <td></td>
                                                    <td></td>
                                                   
                                                </tr>
                                                    }
                                                    reqCompanyId = item.RequiredByCompanyId;
                                                    SterlingPkgId = item.SterlingPkgId;

                                                }
                                            }

                                        </tbody>
                                    </table>
                                </div>
                                }

                                if (Model.IsLicInspply)
                                {
                                    reqCompanyId = string.Empty;
                                <div class="box background-check">
                                    <div class="row-fluid">
                                        <div class="span12">
                                            <h3>Professional License</h3>
                                            <hr />
                                        </div>


                                    </div>
                                    <table id="tbl_ProfLic" class="table table-hover insurance-table">
                                        <thead>
                                            <tr>
                                                                                              
                                                    <th>Required By</th>                                               
                                                
                                                <th>Type</th>
                                                <th>License Number</th>
                                                <th>State</th>
                                                <th>Exp Date</th>
                                                <th>Source</th>
                                                <th>Doc & Data</th>
                                                
                                                <th>Confirmation</th>
                                                <th></th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            @if (!object.Equals(Model.ProfessionalLicense, null))
                                            {
                                                foreach (var item in Model.ProfessionalLicense)
                                                {
                                                <tr>
                                                    
                                                        @if (reqCompanyId != item.RequiredByCompanyId)
                                                        {
                                                        <td style="width: 25%;">@item.ReqCompanyName</td>
                                                        }

                                                    

                                                    <td>@item.LicenseType</td>
                                                    <td>
                                                        @if (!string.IsNullOrEmpty(item.LicenseNumber))
                                                        {
                                                            @item.LicenseNumber
                                                        }
                                                        else
                                                        {<span style="margin-left: 45px;">-</span>}
                                                    </td>
                                                    <td>@if (!string.IsNullOrEmpty(item.StateName))
                                                        {@item.StateName }
                                                        else
                                                        {<span style="margin-left: 15px;">-</span>}</td>
                                                    <td>@if ((item.ExpirationDate != null))
                                                        {<span>@item.ExpirationDate.Value.ToShortDateString()</span>}
                                                        else
                                                        {<span style="margin-left: 25px;">-</span>}</td>
                                                    <td>@item.Source</td>
                                                    <td style="text-align: center;">
                                                        @if (item.Status == "Pass")
                                                        {
                                                            <img src="@Url.Content("~/Content/images/check-in.png")" />
                                                        }
                                                        else
                                                        {
                                                            <img src="@Url.Content("~/Content/images/delete.png")" />
                                                        }
                                                    </td>
                                                    
                                                        <td style="text-align: center;">
                                                            <img src="@Url.Content("~/Content/images/delete.png")" />

                                                        </td>

                                                    
                                                    <td><a style="font-style: italic;" onclick="EditProfLic('@item.LicenseNumber','@item.LicenseType','@item.StateId','@item.ExpirationDate','@item.LicInsId','@item.LicInsMapId')">edit/view</a></td>
                                                </tr> 
                                                    reqCompanyId = item.RequiredByCompanyId;
                                                }

                                            }

                                        </tbody>
                                    </table>
                                </div>


                                }
                                if (Model.IsLicInspply)
                                {
                                    reqCompanyId = string.Empty;
                                <div class="box background-check">
                                    <div class="row-fluid">
                                        <div class="span12">
                                            <h3>Insurance</h3>
                                        </div>
                                    </div>
                                    <table id="tbl_Insurance" class="table table-hover insurance-table">
                                        <thead>
                                            <tr>
                                                                                              
                                                    <th>Required By</th>                                               
                                                
                                                <th>Type</th>
                                                <th>Company Name</th>
                                                <th>Policy Number</th>
                                                <th style="border: solid 1px rgba(0, 0, 0, 0.4); padding: 8px 2px; position: relative; display: block;">
                                                    <div id="div_twoColumnHead" class="twocolumn-heading" style="width: 194px;">Coverage amounts</div>
                                                    Single Occurrence</th>
                                                <th style="border: solid 1px rgba(0, 0, 0, 0.4); padding: 8px 2px;">Aggregate</th>
                                                <th>Exp Date</th>
                                                <th>Doc & Data</th>
                                               
                                                 <th>Confirmation</th>
                                                <th></th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            @if (!object.Equals(Model.ProfessionalInsurance, null))
                                            {
                                                foreach (var item in Model.ProfessionalInsurance)
                                                {
                                                <tr>
                                                   
                                                        @if (reqCompanyId != item.RequiredByCompanyId)
                                                        {
                                                        <td>@item.ReqCompanyName</td>
                                                        }

                                                    
                                                    <td>@item.InsuranceType</td>
                                                    <td>@if (!string.IsNullOrEmpty(item.CompanyName))
                                                        {@item.CompanyName}
                                                        else
                                                        {<span style="margin-left: 25px;">-</span>}</td>
                                                    <td>@if (!string.IsNullOrEmpty(item.PolicyNumber))
                                                        {@item.PolicyNumber}
                                                        else
                                                        {<span style="margin-left: 25px;">-</span>}</td>
                                                    <td>@item.SingleOccurance</td>
                                                    <td>@item.Aggregate</td>
                                                    <td>@if ((item.ExpirationDate != null))
                                                        {<span>@item.ExpirationDate.Value.ToShortDateString()</span>}
                                                        else
                                                        {<span style="margin-left: 20px;">-</span>}</td>
                                                    <td style="text-align: center;">
                                                        @if (item.Status == "Pass")
                                                        {
                                                            <img src="@Url.Content("~/Content/images/check-in.png")" />
                                                        }
                                                        else
                                                        {
                                                            <img src="@Url.Content("~/Content/images/delete.png")" />
                                                        }
                                                    </td>
                                                   
                                                        <td style="text-align: center;">
                                                            <img src="@Url.Content("~/Content/images/delete.png")" />

                                                        </td>

                                                    
                                                    <td><a style="font-style: italic;" onclick="EditIns('@item.PolicyNumber','@item.CompanyName','@item.ExpirationDate','@item.InsuranceType','@item.LicInsId','@item.SingleOccurance','@item.Aggregate','@item.LicInsMapId')">edit/view</a></td>
                                                </tr>
                                                    reqCompanyId = item.RequiredByCompanyId;
                                                }
                                            }

                                        </tbody>
                                    </table>
                                </div>

                                }
                            }
                          
                                <div class="box background-check">
                                    <div class="row-fluid">
                                        <div class="span12">
                                            <h3>Communications</h3>
                                            <hr />
                                        </div>
                                    </div>
                                    <table id="tbl_Communication" class="table table-hover insurance-table">
                                        <thead>
                                            <tr>
                                                <th>Required By</th>
                                                <th>Title</th>
                                                <th>Completion Date</th>
                                                <th>Exp Date</th>
                                                <th>Status</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            @if (!object.Equals(Model.ProfessionalCommunications, null))
                                            {
                                                foreach (var item in Model.ProfessionalCommunications)
                                                {
                                                <tr>

                                                    <td>@item.Docfrom</td>


                                                    <td>@item.DocTitle</td>

                                                    <td>@if ((item.completeDate != null))
                                                        {<span>@item.completeDate.Value.ToShortDateString()</span>}
                                                        else
                                                        {<span style="margin-left: 40px;">-</span>}</td>

                                                    <td>@if ((item.DueDate != null))
                                                        {<span>@item.DueDate.Value.ToShortDateString()</span>}
                                                        else
                                                        {<span style="margin-left: 22px;">-</span>}</td>
                                                    <td>
                                                        @if (item.status == "Pass")
                                                        {
                                                            <img src="@Url.Content("~/Content/images/check-in.png")" />
                                                        }
                                                        else
                                                        {
                                                            <img src="@Url.Content("~/Content/images/delete.png")" />
                                                        }
                                                    </td>


                                                </tr>  
                                                }
                                            }

                                        </tbody>
                                    </table>

                                </div>
                                <div class="box background-check">
                                    <div class="row-fluid">
                                        <div class="span12">
                                            <h3>Additional Requirements</h3>
                                            <hr />
                                        </div>
                                    </div>
                                    <table id="tbl_AddReq" class="table table-hover insurance-table">
                                        <thead>
                                            <tr>
                                                <th>Required By</th>
                                                <th>Title</th>
                                                <th>Completion Date</th>
                                                <th style="text-align: center">Status</th>
                                                <th></th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            @if (!object.Equals(Model.AdditionalRequirements, null))
                                            {
                                                reqCompanyId = string.Empty;
                                                foreach (var item in Model.AdditionalRequirements)
                                                {
                                                <tr>

                                                    <td>@item.ReqCompanyName</td>


                                                    <td>@item.title</td>

                                                    <td>@if ((item.CompletedDate != null))
                                                        {<span>@item.CompletedDate.Value.ToShortDateString()</span>}
                                                        else
                                                        {<span>-</span>}</td>
                                                    <td style="text-align: center">
                                                        @if (item.Status == "Pass")
                                                        {
                                                            <img src="@Url.Content("~/Content/images/check-in.png")" />
                                                        }
                                                        else
                                                        {
                                                            <img src="@Url.Content("~/Content/images/delete.png")" />
                                                        }
                                                    </td>

                                                    <td><a style="font-style: italic;" onclick="EditAdditionalReq('@item.title','@item.UploadedDoc','@item.LicInsId','@item.LicInsMapId')">edit/view</a></td>
                                                </tr>  
                                                        reqCompanyId = item.RequiredByCompanyId;
                                                }
                                            }

                                        </tbody>
                                    </table>
                                </div>
                            
                        </div>
                    </div>
                        
                            
                    <br/>
                }
            </div>
