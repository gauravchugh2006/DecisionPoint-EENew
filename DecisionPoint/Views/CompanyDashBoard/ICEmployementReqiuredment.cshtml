<script>
    var ROOT = '@Url.Content("~/")';
    $(document).ready(function () {
        ShowHideTabs('administration');

        $('#step1content').removeClass("active2");
        $('#step2recipient').addClass("active2");
        
        //add required action for global
        AddAnotherglobalsection();
        //add required action for specific
        AddAnotherspcificsection();
        var count = $('#tblservice td').length;
        if (count == 1) {
            $('#tblservice').css("width", "auto");
        }
        else {
            var width = parseInt(count) * 154;
            $('#tblservice').css("width", width);
        }

        if ('@Gcount' > 0) {
            $('#g_0').hide();
        }
        if ('@Scount' > 0) {
            $('#s_0').hide();
        }

        $('#cglobaltable').addClass('pull-left');
        $('#cspecifictable').addClass('pull-left');

    })
    $('#btnClosePopup').click(function () {
        $('#EditCsv').hide();
        $('#popupCSV').hide();
        Rsetdiv("E");
    })
    function AddAnotherglobalsection() {

        $("#addotherglobaltitle").click(function () {
            if ($('#g_0').hide()) {
                $('#g_0').show();
                $('#gchkdonotshow_0').prop("checked", true);
                $('#glbltitle_0').removeAttr("disabled");
                $('#addotherglobaltitle').hide();
            }

        })
    }
    function AddAnotherspcificsection() {
        $("#addotherspcifictitle").click(function () {
            if ($('#s_0').hide()) {
                $('#s_0').show();
                $("#addotherspcifictitle").hide();
                $('#schkdonotshow_0').prop("checked", true);
                $('#servicesection').show();
                $('#slbltitle_0').removeAttr("disabled");
            }


        })
    }

    function ShowTitle(ReqDocId, title, IsCompanyReq, IsLicenseReq, IsPolicyReq, IsStateReq, IsExpDateReq, UserPer, reqdocfor, DoNotShow, service, JCRTYPE, Retake) {

        $('#EditCsv').show();
        $('#popupCSV').show();
        $('#tblVType .chkVType').each(function () {
            $(this).prop("checked", false);
        })
        $('#VTypesection').hide();
        $('.VtypeEdit').html($('#VTypesection').clone().html());

        $('#lnkviewVType').html("Show Profession Type");
        var scrollTop = (window.pageYOffset !== undefined) ? window.pageYOffset : (document.documentElement || document.body.parentNode || document.body).scrollTop;
        var poistion = scrollTop + 100;
        $('#popupCSV').css("top", poistion);

        if (reqdocfor == "staff") {
            $('#chkstaffedit').prop("checked", true);
        }

        if (reqdocfor == "ic") {
            $('#chkICedit').prop("checked", true);
            $('#lnkviewVType').show();
        }
        if (reqdocfor == "both") {
            $('#lnkviewVType').show();
            $('#chkICedit').prop("checked", true);
            $('#chkstaffedit').prop("checked", true);
        }
        $("#Eretake option").each(function () {

            $(this).removeAttr('selected');

        });

        $("#Eretake option").each(function () {
            if ($(this).val() == Retake) {
                $(this).attr('selected', 'selected');
            }
        });
        // $("#Eretake :selected").val(Retake);
        //get vendor type details
        $.ajax({
            url: '@Url.Content("~/CompanyDashBoard/GetReqDocVendorTypeByreqDocId/")',
            data: { 'reqDocId': parseInt(ReqDocId) },
            cache: false,
            type: "GET",
            success: function (result) {
                //code to bind table
                result = $.parseJSON(result);
                $.each(result, function (i, item) {
                    $('.VtypeEdit .chkVType').each(function () {
                        if ($(this).val() == item) {
                            //alert('come');
                            $(this).prop("checked", true);
                        }
                    })
                });

            }
        });

        $.ajax({
            url: '@Url.Content("~/CompanyDashBoard/GetAckByreqDocId/")',
            data: { 'reqDocId': parseInt(ReqDocId) },
            cache: false,
            type: "GET",
            success: function (result) {

                result = $.parseJSON(result);
                //code to bind table
                $.each(result, function (i, item) {
                    var id = "";
                    id = $('#Etbreqaction_0 tr').length;
                    $('#Etbreqaction_0 tr:last').after('<tr><td><input id="Echk_' + id + '" type="checkbox" style="margin-top:-13px;" checked="checked" /> </td>' +
              '<td><input id="Ereqcon_' + id + '" class="span12" placeholder="Add Reqiured Action..." type="text" value="' + item + '" maxlength="250" style="margin-top:-1px;" /></td>' +
              '<td><img id="Edelaction_' + id + '"  src="@Url.Content("~/Content/images/cross-button.png")" style="margin-left:6px;cursor:pointer;margin-top:-11px;" title="Remove Ack" onclick="RemoveAction(this.id)">' +
                    '<input type="hidden" id="reqconid_' + id + '" value="0"/></td></tr>');

                });
            }
        });
        $.ajax({
            url: '@Url.Content("~/CompanyDashBoard/GetUploadDocByreqDocId/")',
            data: { 'reqDocId': parseInt(ReqDocId) },
            cache: false,
            type: "GET",
            success: function (result) {

                $('#Edocfiles_0').html("");
                result = $.parseJSON(result);

                var count = "";
                var fileurl = "";
                var url1 = "";
                $.each(result, function (i, item) {

                    count = $('#Edocfiles_0 div').length;
                    url1 = item.DocLoc;

                    $("#Edocfiles_0").append('<div id="div_' + count + '" style="border-radius: 5px 5px 5px 5px;  width:auto;height:auto;float:left;"><a id="docfile_' + count + '" href="' + url1 + '" target="_blank" title=' + title + '_' + item.DocSeq + '<style="cursor:pointer;margin-left:6px;" >' + title.replace(/\s/g, '') + '_' + item.DocSeq + '</a><img id="Eimg' + count + '" src="@Url.Content("~/Content/images/cross-button.png")" style="margin-left:6px;cursor:pointer;" title="remove document" onclick="deletedoc(this.id)">' +
                                   '<input type="hidden" id="doccontentid_' + count + '" value="' + item.DocTblId + '" /></div>');

                })
            }
        });

        $('#Elbltitle_0').val(title);
        $('#Elbltitle_0').attr("title", title);
        if (!DoNotShow) {
            if (UserPer) {
                $('#Echkaupload_0').prop('checked', true)
                $('#Echkaview_0').prop('checked', false)
            }
            else {
                $('#Echkaview_0').prop('checked', true)
                $('#Echkaupload_0').prop('checked', false)
            }
        } else {
            $('#Echkdonotshow_0').prop('checked', DoNotShow)
        }
        //JCR TYpe
        $('#Elbltitle_0').removeAttr("disabled");
        // alert(JCRTYPE);
        $('#JCRTYPE').val(JCRTYPE);
        //set service value
